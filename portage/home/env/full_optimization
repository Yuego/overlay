CFLAGS="-O3 -march=native -pipe -mmmx -mssse3 -fomit-frame-pointer"
CFLAGS="${CFLAGS} --param l1-cache-size=32 --param l1-cache-line-size=64 --param l2-cache-size=2048"
CFLAGS="${CFLAGS} -fno-align-functions -fno-align-loops -fno-align-labels -fno-align-jumps"
CFLAGS="${CFLAGS} -ffast-math"
CFLAGS="${CFLAGS} -fno-gcse --param max-unroll-times=4 --param max-unrolled-insns=72 --param max-average-unrolled-insns=216 -funroll-all-loops"
CFLAGS="${CFLAGS} -g0 -Wno-all"
CXXFLAGS="${CFLAGS}"
